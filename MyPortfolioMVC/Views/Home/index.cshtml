                        </div>
                    </div>
                </form>
                <div class="row" style="text-align: center">
                    <button type="button" class="btn btn-success" id="result" style="font-size: 18px;">Factorial!</button>
                </div>
            </div>

            <div class="row" id="special" style="background-color: lightseagreen; margin-right: 0px; margin-left: 0px;">
                <div class="col-md-12" style="text-align: center">
                    <h2 id="chao">Result:</h2>
                    <p id="answer1"></p>
                </div>

                <div class="row" style="text-align:center">
                    <button type="button" id="btnFactorial" class="btn btn-primary btn-md" data-toggle="modal" data-target="#factorial"  title="Factorial" data-placement="auto" style="font-size: 18px;">View Factorial-code</button>
                </div>
            </div>
            <div class="row" id="factorialCode" style="margin-left:0px; margin-right: 0px;background-color:cornflowerblue">
                <pre id="facpretag" class="brush: js" style="padding: 10px">
                <!--FACTORIAL-APPLICATION-->
$("#result").click(function () {
        var num = parseInt($("#inputNumber").val());
        if (num == 0) {
            return document.getElementById("answer1").innerHTML = 1;
        }
        else if (/\./.test(num)) {
            $("#answer1").html("Please enter an integer number");
        }
        else if (num > 150) {
            $("#answer1").html("The number you have entered is too large. Please keep it between 1 and 150.");
        }
        else {
            var number = 1;
            for (var i = num; i > 0; i--) {
                number *= i;
            }
        }
        $("#answer1").html("The factorial number is: " + number);
});
$("#cls").click(function () {
        $("#inputNumber, #answer1").each(function () {
            $(this).val("");
            $(this).empty();
        })
})
                                        </pre>
            </div>

            <div class="modal-footer">
                <button type="button" style="float: left" class="btn btn-primary" id="cls">Clear(s)</button>
                <button type="button" class="btn btn-success btn-lg" style="padding: 5px 20px 5px 20px;" data-dismiss="modal">Close</button>
            </div>
