                    <button type="button" id="btnPalindrome" class="btn btn-primary btn-md" data-toggle="modal" data-target="#pal"  title="Palindrome" data-placement="auto">View Palindrome-code</button>
                </div>
                <div class="row" id="palindromeCode" style="background-color:cornflowerblue">
                    <pre id="palpretag" class="brush: js" style="padding: 10px">
                <!--PALINDROME-APPLICATION-->
$("#submit").click(function () {
        var str = $("#typeWords").val();
        str = str.toLowerCase();
        str = str.replace(/[\W_]/g, "");
        var inputStr1 = str.split("").reverse().join("");
        if (inputStr1 == str) {
            $("#answer").html("Yes, " + str + " IS a palindrome!")
        }
        else {
            $("#answer").html("No, " + str + " IS NOT a palindrome!")
        }
});
    $("#btnClearPal").click(function () {
        $("#typeWords, #answer ").each(function () {
            $(this).val("");
            $(this).html("");
        })
})
